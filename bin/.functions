function goto() {
	local project=${1}
	declare -A WORKSPACE
	WORKSPACE['1']="${HOME}/workspace"
	WORKSPACE['2']="/mnt/git-repository"
	for workspace in $(echo ${WORKSPACE[@]}|sort);do
		if [ -d ${workspace}/${project} ] ; then
			cd ${workspace}/${project}
			break
		fi
	done
}

function displayit() {
	local msg=$*
	echo ${msg} 1>&2
}

function source_sshagentpid() {
	. ${HOME}/.sshagentpid.$(hostname -s)
}

function which() {
	(alias; declase -f) | /usr/bin/which --tty-only --read-alias --read-functions --show-tilde --show-dot $@
}